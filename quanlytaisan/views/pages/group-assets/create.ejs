<div id="wrapper">
    <div class="container-fluid">
        <h6 class="title-header">Thêm mới nhóm tài sản</h6>
        <form action="/group-assets/create" method="POST">
            <div class="form-group row">
                <label class="col-md-4 col-form-label text-md-right">Tên nhóm tài sản</label>
                <div class="col-md-4">
                    <input type="text" id="full_name" min="1" maxlength="255" required autofocus="autofocus"
                        class="form-control" name="full_name">
                    <!-- <span th:if="${#fields.hasErrors('groupName')}" th:errors="*{groupName}" class="text-danger"></span> -->
                </div>
            </div>

            <div class="col-md-6 offset-md-4">
                <button type="submit" class="btn btn-primary">
                    Tạo nhóm tài sản
                </button>
            </div>
        </form>
    </div>
    <!-- /.container-fluid -->
</div>

<% for(var i=0 ;i< products.length;i++) {%>
<tr>
  <td><input type="checkbox" class="form-check-inline" /></td>
  <td>
    <img
      src="images/capp3.jpg"
      width="50%"
      class="img-fluid"
      alt=""
    />
  </td>
  <td><%= products[i].name%></td>
  <td><%= products[i].quantity%></td>
  <td><%= products[i].unit_price%></td>
  <td><%= products[i].discount%></td>
  <td><%= products[i].status%></td>
  <td><%= products[i].category_name%></td>

  <td>
    <a href="/items/products/${products[i]._id}" class="btn btn-outline-info"
      ><i class="fas fa-info"></i
    ></a>
    <a
      href="/items/products?id=<%= products[i]._id%>"
      class="btn btn-outline-warning"
      ><i class="fas fa-edit"></i
    ></a>
    <a
      onclick="showConfirModal(this.getAttribute('data-id'),this.getAttribute('data-name'))"
      class="btn btn-outline-danger" data-name=<%= products[i].name%>
      data-id=<%= products[i]._id%>
    >
      <i class="fas fa-trash-alt"></i
    ></a>
  </td>
</tr>
<%}%>
